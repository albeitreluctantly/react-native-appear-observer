{
  "compilerOptions": {
    "incremental": true,          
    "target": "es2022",              
    "module": "ES2022",
    "moduleResolution": "nodenext",          
    "declaration": true,                    
    "outDir": "dist",               
    "esModuleInterop": true,      
    "skipLibCheck": true,        
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-native",
    "noEmitHelpers": true,              
  }
}